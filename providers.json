{
  "version": "1.0",
  "providers": [
    {
      "name": "copilot_codex",
      "priority": 1,
      "type": "api",
      "adapter": "copilot_api",
      "rate_limit_strategy": "detect_http_429",
      "confidence_weight": 1.0,
      "enabled": true,
      "config": {
        "api_key_env": "COPILOT_API_KEY",
        "model": "codex-4",
        "timeout_seconds": 120
      }
    },
    {
      "name": "claude_cli",
      "priority": 2,
      "type": "cli",
      "adapter": "claude_cli",
      "rate_limit_strategy": "error_pattern",
      "confidence_weight": 0.95,
      "enabled": true,
      "config": {
        "executable": "claude",
        "rate_limit_patterns": [
          "rate limit",
          "too many requests",
          "quota exceeded"
        ],
        "timeout_seconds": 180
      }
    },
    {
      "name": "goose_ollama",
      "priority": 3,
      "type": "local",
      "adapter": "goose",
      "rate_limit_strategy": "none",
      "confidence_weight": 0.85,
      "enabled": true,
      "config": {
        "executable": "goose",
        "model": "qwen2.5-coder:14b",
        "timeout_seconds": 300
      }
    },
    {
      "name": "ollama_direct",
      "priority": 4,
      "type": "local",
      "adapter": "ollama",
      "rate_limit_strategy": "none",
      "confidence_weight": 0.80,
      "enabled": false,
      "config": {
        "base_url": "http://localhost:11434",
        "model": "codellama:13b",
        "timeout_seconds": 300
      }
    }
  ]
}
